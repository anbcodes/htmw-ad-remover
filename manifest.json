{

  "manifest_version": 2,
  "name": "HTMW Ad Hider",
  "version": "1.3",

  "description": "Hides ads on HowTheMarketWorks.com",

  "icons": {
    "48": "icons/cancel.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.howthemarketworks.com/*"],
      "css": ["removeads.css"]
    }
  ]

}